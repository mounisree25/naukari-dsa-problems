class Node
{
int data;
Node left;
Node right;
 Node(int data)
{
this.data=data;
this.left=null;
this.right=null;
}
}
import java.util.*;
class Solution
{
public static int floorinbst(Node root,int x)
{
int floor=-1;
while(root!=null)
{
if(root.data<x)
{
floor=root.data;
root=root.right;
}
else
{
root=root.left;
}
}
return floor;
}
}
